{
  "name": "roc-web",
  "version": "1.0.0-alpha10",
  "description": "Web base extension for Roc Applications",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src/roc --out-dir lib --source-maps",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf lib docs",
    "test": "npm run lint && npm run test:coverage",
    "test:unit": "mocha --compilers js:babel/register --recursive test/",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:coverage": "babel-node ./node_modules/.bin/isparta cover _mocha -- --recursive test/",
    "lint": "eslint .",
    "check": "ncu",
    "docs": "esdoc -c esdoc.json",
    "prepublish": "npm run lint && npm run clean && npm run build && npm run docs && npm run test:unit"
  },
  "keywords": [
    "roc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vgno/roc-web.git"
  },
  "devDependencies": {
    "babel": "~5.8.23",
    "babel-eslint": "~4.1.3",
    "chai": "~3.4.0",
    "esdoc": "~0.4.3",
    "esdoc-es7-plugin": "~0.0.3",
    "esdoc-importpath-plugin": "0.0.1",
    "eslint": "~1.7.3",
    "eslint-config-vgno": "~4.0.1",
    "mocha": "~2.3.3",
    "isparta": "~3.5.3",
    "codacy-coverage": "~1.1.3",
    "codeclimate-test-reporter": "~0.1.1",
    "coveralls": "~2.11.4",
    "npm-check-updates": "~2.3.4"
  },
  "roc": "1.0.0-alpha8",
  "author": "VG",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "~6.1.2",
    "babel-loader": "~5.3.2",
    "bourbon": "~4.2.6",
    "bourbon-neat": "~1.7.2",
    "browser-sync": "~2.9.11",
    "colors": "~1.1.2",
    "config": "~1.16.0",
    "css-loader": "~0.23.0",
    "debug": "~2.2.0",
    "deep-assign": "~2.0.0",
    "dev-ip": "~1.0.1",
    "extract-text-webpack-plugin": "~0.8.2",
    "file-loader": "~0.8.4",
    "json-loader": "~0.5.3",
    "koa": "~1.1.1",
    "koa-accesslog": "~0.0.2",
    "koa-compressor": "~1.0.3",
    "koa-conditional-get": "~1.0.3",
    "koa-etag": "~2.0.0",
    "koa-favicon": "~1.2.0",
    "koa-helmet": "~0.3.0",
    "koa-logger": "~1.3.0",
    "koa-static": "~1.5.1",
    "koa-webpack-dev-middleware": "~1.0.1",
    "koa-webpack-hot-middleware": "~1.0.1",
    "mkdirp": "~0.5.1",
    "multi-progress": "~1.0.0",
    "node-sass": "~3.4.2",
    "node-watch": "~0.3.4",
    "postcss-loader": "~0.8.0",
    "prettysize": "0.0.3",
    "rimraf": "~2.4.3",
    "roc-config": "*",
    "sass-loader": "~3.1.2",
    "source-map-support": "~0.3.3",
    "style-loader": "~0.13.0",
    "url-loader": "~0.5.6",
    "webpack": "~1.12.2",
    "webpack-hot-middleware": "2.x"
  }
}
